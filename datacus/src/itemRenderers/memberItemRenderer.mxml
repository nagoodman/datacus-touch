<?xml version="1.0" encoding="utf-8"?>
<s:ItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009" 
				xmlns:s="library://ns.adobe.com/flex/spark" 
				xmlns:mx="library://ns.adobe.com/flex/mx" 
				autoDrawBackground="false"
				xmlns:local="*" height="100" width="165" >
	

		<s:states>
			<s:State name="normal" />
			<s:State name="down" />
			<s:State name="selected"/>
		</s:states>
		<s:Group x="-1" y="0">
			<s:layout>
				<s:HorizontalLayout verticalAlign="middle"
									paddingLeft="0"/>
			</s:layout>
			<s:SkinnableContainer id="anim8" width="149" height="90">
				<local:level  x="5" y="0"/>
				
				<s:Label text="{data.label}" color="#dddddd" width="100%" height='100%' textAlign="center" fontSize="16" verticalAlign="middle" x="10" y="0"/>
			</s:SkinnableContainer>
			
		</s:Group>

	
</s:ItemRenderer>
